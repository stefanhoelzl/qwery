{
  "name": "@qwery/legacy",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "DB=src/project/data.ddb vite dev",
    "build": "vite build",
    "preview": "DB=src/project/data.ddb vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "format": "prettier --write . && eslint . --fix --max-warnings=0",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "tsx": "npx tsx",
    "build:image": "buildah build --output=type=tar,dest=image.tar -t qwery ."
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.16.0",
    "@testing-library/svelte": "^5.2.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.9",
    "clsx": "^2.1.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "@qwery/vite-plugin": "file:../vite-plugin"
  },
  "dependencies": {
    "@qwery/dashboard": "file:../dashboard"
  }
}
